{
  "name": "ngx-formwork",
  "version": "0.6.0",
  "scripts": {
    "build": "tsc -p tsconfig.schematics.json",
    "postbuild": "copyfiles schematics/*/schema.json schematics/*/files/** schematics/collection.json ../../dist/core/ && copyfiles -f schematics/ng-add/files/formwork.config.ts.template ../../dist/core/schematics/ng-add/files/"
  },
  "peerDependencies": {
    "@angular/cdk": "^19.2.1",
    "@angular/common": "^19.2.1",
    "@angular/core": "^19.2.1"
  },
  "dependencies": {
    "acorn": "^8.14.1",
    "eval-estree-expression": "^3.0.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular-devkit/schematics": "^19.2.9",
    "@schematics/angular": "^19.2.9",
    "copyfiles": "file:../../node_modules/copyfiles",
    "typescript": "file:../../node_modules/typescript"
  },
  "schematics": "./schematics/collection.json",
  "ng-add": {
    "save": true
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "license": "MIT",
  "keywords": [
    "angular",
    "typescript",
    "reactive-forms",
    "json-form"
  ],
  "author": {
    "name": "Alexander Pahn",
    "email": "alexander.pahn@outlook.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TheNordicOne/ngx-formwork.git"
  },
  "bugs": {
    "url": "https://github.com/TheNordicOne/ngx-formwork/issues"
  }
}
